{
  "manifest_version": 3,
  "name": "Button Adder",
  "version": "1.0",
  "description": "A simple Chrome extension to add a button to a page.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
